<main>
  <%= button_to("Your Stories", author_stories_path(current_author), method: :get) if !is_admin? %>

  <article class="story-card">
    <h1><%= @story.story_title %></h1>
    <p><%= @story.story %></p>
    <%= button_to("Edit", edit_author_story_path(@story), method: :get) if !is_admin? %>
  </article>

</main>
